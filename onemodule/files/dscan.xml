        <paragraphe valeur_ptsodj="1" ordinal_prise="1" id_preparation="1122712" ordre_absolu_seance="7" id_acteur="PA332747" id_mandat="PM722798" id_nomination_oe="-1" id_nomination_op="PM730930" code_grammaire="ODJ_APPEL_DISCUSSION" code_style="NORMAL" code_parole="" sommaire="0" id_syceron="1386208" valeur=" (n[[os]] 911, 1053, 1097, 1098, 1137)" roledebat="president">
          <orateurs>
